            </xosid:return>
            <xosid:compliance xosid:type="mandatory">
                <xosid:description>
                    This method must be implemented.
                </xosid:description>
            </xosid:compliance>
        </xosid:method>

        <xosid:method xosid:name="getHierarchy">
            <xosid:description>
                Gets the <xosid:token>Hierarchy</xosid:token>
                associated with this session.
