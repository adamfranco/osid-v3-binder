            <xosid:return>
                <xosid:interfaceType xosid:type="osid.hierarchy.Hierarchy" xosid:array="true"/>
                <xosid:description>
                    an array of <xosid:token>Hierarchy
                   </xosid:token>elements. <xosid:token>
                    </xosid:token>The length of the array is less than
                    or equal to the number specified.
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="ILLEGAL_STATE" xosid:category="Programming">
                <xosid:description>
