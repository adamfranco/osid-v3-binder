    <xosid:interface xosid:name="osid.course.CourseCatalogList"> 
        <xosid:implements xosid:interface="osid.OsidList"/>
        <xosid:description>
            Like all <xosid:token>OsidLists,</xosid:token>
            <xosid:token>CourseCatalogList</xosid:token> provides a
            means for accessing
            <xosid:token>CourseCatalog</xosid:token> elements
            sequentially either one at a time or many at a time. Examples:

            <xosid:pbreak/>

<xosid:code>
<![CDATA[
while (ccl.hasNext()) {
     CourseCatalog catalog = ccl.getNextCourseCatalog();
}

]]>
            </xosid:code>
            or
            <xosid:code>
<![CDATA[


while (ccl.hasNext()) {
     CourseCatalog[] catalogs = ccl.getNextCourseCatalog(ccl.available());
}]]>
            </xosid:code>
             
        </xosid:description>


        <xosid:method xosid:name="getNextCourseCatalog">
            <xosid:description>
                Gets the next <xosid:token>CourseCatalog</xosid:token>
                in this list.
            </xosid:description>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.course.CourseCatalog"/>
                <xosid:description>
                    the next <xosid:token>CourseCatalog</xosid:token>
                    in this list. The
                    <xosid:token>hasNext()</xosid:token> method should
                    be used to test that a next
                    <xosid:token>CourseCtalog</xosid:token> is
                    available before calling this method.
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="ILLEGAL_STATE" xosid:category="Integration">
                <xosid:description>
                    no more elements available in this list
                </xosid:description>
            </xosid:error>
            <xosid:error xosid:type="OPERATION_FAILED" xosid:category="Operational">
                <xosid:description>
                    unable to complete request
                </xosid:description>
            </xosid:error>
            <xosid:compliance xosid:type="mandatory">
                <xosid:description>
                    This method must be implemented.
                </xosid:description>
            </xosid:compliance>
        </xosid:method>

        <xosid:method xosid:name="getNextCourseCatalogs">
            <xosid:description>
                Gets the next set of <xosid:token>CourseCatalog
                </xosid:token>elements in this list. The specified
                amount must be less than or equal to the return from
                <xosid:token>available().</xosid:token>
            </xosid:description>
            <xosid:parameter xosid:name="n">
                <xosid:primitiveType xosid:type="cardinal"/>
                <xosid:description>
                    the number of <xosid:token>CourseCatalog
                    </xosid:token>elements requested which must be less
                    than or equal to
                    <xosid:token>available()</xosid:token>
                </xosid:description>
            </xosid:parameter>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.course.CourseCatalog" xosid:array="true"/>
                <xosid:description>
                    an array of <xosid:token>CourseCatalog
                   </xosid:token>elements. <xosid:token>
                    </xosid:token>The length of the array is less than
                    or equal to the number specified.
