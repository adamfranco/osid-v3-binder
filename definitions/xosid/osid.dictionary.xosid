            </xosid:parameter>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.dictionary.Entry" xosid:array="true"/>
                <xosid:description>
                    an array of <xosid:token>Entry
                   </xosid:token>elements. <xosid:token>
                    </xosid:token>The length of the array is less than
                    or equal to the number specified.
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="ILLEGAL_STATE" xosid:category="Programming">
                <xosid:description>
