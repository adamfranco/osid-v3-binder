                                        <xosid:token>EntryNotificationSession:</xosid:token>
                    a session for subscribing to notifications about
                    dictionary entries
                </xosid:element>
                 <xosid:element>
                    <xosid:token>DictionaryLookupSession</xosid:token>
                    a session for looking up dictionaries
                </xosid:element>
                 <xosid:element>
                    <xosid:token>DictionarySearchSession</xosid:token>
                    a session for searchingamong dictionaries
                </xosid:element>
                 <xosid:element>
                    <xosid:token>DictionaryAdminSession</xosid:token> a
                    session creating, updating or deleting dictionaries
                </xosid:element>
                 <xosid:element>

                                        <xosid:token>DictionaryNotificationSession:</xosid:token>
                    a session for subscribing to adds and changes of dictionaries
                </xosid:element>
                 <xosid:element>

                                        <xosid:token>DictionaryHierarchySession:</xosid:token>
                    a session for traversing the hierarchy of dictionaries
                </xosid:element>
