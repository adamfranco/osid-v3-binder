    <xosid:interface xosid:name="osid.repository.AssetContentList"> 
        <xosid:implements xosid:interface="osid.OsidList"/>
        <xosid:description>
            Like all <xosid:token>OsidLists,</xosid:token>
            <xosid:token>AssetContentList</xosid:token> provides a
            means for accessing <xosid:token>AssetContent</xosid:token>
            elements sequentially either one at a time or many at a
            time. Examples:

            <xosid:pbreak/>

<xosid:code>
<![CDATA[
while (acl.hasNext()) {
     AssetContent content = acl.getNextAssetContent();
}

]]>
            </xosid:code>
            or
            <xosid:code>
<![CDATA[


while (acl.hasNext()) {
     AssetContent[] content = acl.getNextAssetContents(acl.available());
}]]>
            </xosid:code>
             
        </xosid:description>


        <xosid:method xosid:name="getNextAssetContent">
            <xosid:description>
                Gets the next <xosid:token>AssetContent</xosid:token>
                in this list.
            </xosid:description>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.repository.AssetContent"/>
                <xosid:description>
                    the next <xosid:token>AssetContent</xosid:token> in
                    this list. The <xosid:token>hasNext()</xosid:token>
                    method should be used to test that a next
                    <xosid:token>AssetContent</xosid:token> is
                    available before calling this method.
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="ILLEGAL_STATE" xosid:category="Programming">
                <xosid:description>
