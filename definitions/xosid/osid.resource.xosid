    <xosid:interface xosid:name="osid.resource.BinList"> 
        <xosid:implements xosid:interface="osid.OsidList"/>
        <xosid:description>
            Like all <xosid:token>OsidLists,</xosid:token>
            <xosid:token>BinList</xosid:token> provides a means for
            accessing <xosid:token>Bin</xosid:token> elements
            sequentially either one at a time or many at a time. Examples:

            <xosid:pbreak/>

<xosid:code>
<![CDATA[
while (bl.hasNext()) {
     Bin bin = bl.getNextBin();
}

]]>
            </xosid:code>
            or
            <xosid:code>
<![CDATA[


while (bl.hasNext()) {
     Bin[] bins = rl.getNextBin(bl.available());
}]]>
            </xosid:code>
             
        </xosid:description>


        <xosid:method xosid:name="getNextBin">
            <xosid:description>
                Gets the next <xosid:token>Bin</xosid:token> in this
                list.
            </xosid:description>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.resource.Bin"/>
                <xosid:description>
                    the next <xosid:token>Bin</xosid:token> in this
                    list. The <xosid:token>hasNext()</xosid:token>
                    method should be used to test that a next
                    <xosid:token>Bin</xosid:token> is available before
                    calling this method.
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="ILLEGAL_STATE" xosid:category="Programming">
                <xosid:description>
                    no more elements available in this list
                </xosid:description>
            </xosid:error>
            <xosid:error xosid:type="OPERATION_FAILED" xosid:category="Operational">
                <xosid:description>
                    unable to complete request
                </xosid:description>
            </xosid:error>
            <xosid:compliance xosid:type="mandatory">
                <xosid:description>
                    This method must be implemented.
                </xosid:description>
            </xosid:compliance>
        </xosid:method>

        <xosid:method xosid:name="getNextBins">
            <xosid:description>
                Gets the next set of <xosid:token>Bin
                </xosid:token>elements in this list which must be less
                than or equal to the return from
                <xosid:token>available().</xosid:token>
            </xosid:description>
            <xosid:parameter xosid:name="n">
                <xosid:primitiveType xosid:type="cardinal"/>
                <xosid:description>
                    the number of <xosid:token>Bin
                    </xosid:token>elements requested which must be less
                    than or equal to
                    <xosid:token>available()</xosid:token>
                </xosid:description>
            </xosid:parameter>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.resource.Bin" xosid:array="true"/>
                <xosid:description>
                    an array of <xosid:token>Bin
                   </xosid:token>elements. <xosid:token>
                    </xosid:token>The length of the array is less than
                    or equal to the number specified.
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="ILLEGAL_STATE" xosid:category="Programming">
                <xosid:description>
                    no more elements available in this list
