        <xosid:method xosid:name="getResourcesBySearch">
            <xosid:description>
                Gets the search results matching the given search query
                using the given search. Each element in the query array
                is OR'd.
            </xosid:description>
            <xosid:parameter xosid:name="resourceQueries">
                <xosid:interfaceType xosid:type="osid.resource.ResourceQuery" xosid:array="true"/>
                <xosid:description>
                    the search query array
                </xosid:description>
            </xosid:parameter>
            <xosid:parameter xosid:name="resourceSearch">
                <xosid:interfaceType xosid:type="osid.resource.ResourceSearch"/>
                <xosid:description>
                    the search interface
                </xosid:description>
            </xosid:parameter>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.resource.ResourceSearchResults"/>
                <xosid:description>
                    the returned search results
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="NULL_ARGUMENT" xosid:category="Programming">
                <xosid:description>
