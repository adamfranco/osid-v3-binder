    <xosid:interface xosid:name="osid.resource.Resource"> 
        <xosid:implements xosid:interface="osid.OsidObject"/>
        <xosid:description>
            A <xosid:token>Resource</xosid:token> represents an
            arbitrary entity. Resources are used to define an object to
            accompany an OSID <xosid:token>Id</xosid:token> used in
            other OSIDs. A resource may be used to represent a meeting
            room in the Scheduling OSID, or a student in the Course
            OSID. 

            <xosid:pbreak/>

            A <xosid:token>Resource</xosid:token> may also represent a
            group or organization. A provider may present such a group
            in an opaque manner through a single resource definition,
            or the provider may expose the resource collection for
            examination or manipulation. If such a resource collection
            is visible, <xosid:token>isGroup()</xosid:token> is
            <xosid:token>true</xosid:token> and can be used in one of
            the group sessions available in this OSID.
        </xosid:description>


        <xosid:method xosid:name="isGroup">
            <xosid:description>
                Tests if this resource is a group. A resource that is a
                group can be used in the group sessions.
            </xosid:description>
            <xosid:return>
                <xosid:primitiveType xosid:type="boolean"/>
                <xosid:description>
                    <xosid:token>true</xosid:token> if this resource is
                    a group, <xosid:token>false</xosid:token> otherwise
                </xosid:description>
            </xosid:return>
            <xosid:compliance xosid:type="mandatory">
                <xosid:description>
                    This method must be implemented.
                </xosid:description>
            </xosid:compliance>
        </xosid:method>

        <xosid:method xosid:name="getResourceRecord">
            <xosid:description>
                Gets the record corresponding to the given
                <xosid:token>Resource</xosid:token> record
                <xosid:token>Type. </xosid:token>This method must be
                used to retrieve an object implementing the requested
                record interface along with all of its ancestor
                interfaces. The
                <xosid:token>resourceRecordType</xosid:token> may be
                the <xosid:token>Type</xosid:token> returned in
                <xosid:token>getRecordTypes()</xosid:token> or any of
                its parents in a <xosid:token>Type</xosid:token>
                hierarchy where
                <xosid:token>hasRecordType(resourceRecordType)</xosid:token>
                is <xosid:token>true</xosid:token>.
            </xosid:description>
            <xosid:parameter xosid:name="resourceRecordType">
                <xosid:interfaceType xosid:type="osid.type.Type"/>
                <xosid:description>
                    the resource record type
                </xosid:description>
            </xosid:parameter>
            <xosid:return>
                <xosid:interfaceType xosid:type="osid.resource.ResourceRecord"/>
                <xosid:description>
                    the resource record
                </xosid:description>
            </xosid:return>
            <xosid:error xosid:type="NULL_ARGUMENT" xosid:category="Programming">
                <xosid:description>
                    <xosid:token>resourceRecordType</xosid:token> is
                    <xosid:token>null</xosid:token>
                </xosid:description>
            </xosid:error>
            <xosid:error xosid:type="OPERATION_FAILED" xosid:category="Operational">
                <xosid:description>
                    unable to complete request
                </xosid:description>
            </xosid:error>
            <xosid:error xosid:type="PERMISSION_DENIED" xosid:category="User">
                <xosid:description>
                    authorization failure occurred
                </xosid:description>
            </xosid:error>
            <xosid:error xosid:type="UNSUPPORTED" xosid:category="Integration">
                <xosid:description>

                                        <xosid:token>hasRecordType(resourceRecordType)</xosid:token>
                    is <xosid:token>false</xosid:token>
                </xosid:description>
            </xosid:error>
            <xosid:compliance xosid:type="mandatory">
                <xosid:description>
                    This method must be implemented.
                </xosid:description>
            </xosid:compliance>
        </xosid:method>
    </xosid:interface>


    <xosid:interface xosid:name="osid.resource.ResourceRecord"> 
