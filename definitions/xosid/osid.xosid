        <xosid:method xosid:name="registerForServiceMessages">
            <xosid:description>
                Register for service messages.
                <xosid:token>ServiceMessage.newMessage()</xosid:token>
                is invoked for each new message. There is a single
                service message receiver per manager.
            </xosid:description>
            <xosid:parameter xosid:name="receiver">
